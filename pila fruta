#ejemplo de uso de cola 
fruits = []
pila = []
# encolemos la fruta
fruits.append("banana")
fruits.append("granada")
fruits.append("mango")
fruits.append("orange")

# retiremos de la cola 
first_item=fruits.pop(0)
pila.append(first_item)
first_item=fruits.pop(0)
pila.append(first_item)

#nuevamente
first_item = fruits.pop(0)
print(first_item)

# que esta
print (fruits)
print (pila)
